[[deployment]]
name = "cronicle"
[deployment.config]
server = "komo-main"
image.type = "Image"
image.params.image = "soulteary/cronicle"
volumes = """
/etc/localtime:/etc/localtime:ro
/etc/timezone:/etc/timezone:ro
/data/cronicle/data:/opt/cronicle/data:rw
/data/cronicle/logs:/opt/cronicle/logs:rw
/data/cronicle/plugins:/opt/cronicle/plugins:rw
"""
